# Alchemy API Key for Sepolia RPC endpoint
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Chainlink VRF Subscription ID (from vrf.chain.link/sepolia)
VRF_SUBSCRIPTION_ID=your_subscription_id_here

# Deployed Raffle contract address
RAFFLE_CONTRACT_ADDRESS=your_raffle_address_here

# Chainlink Automation configuration
AUTOMATION_UPKEEP_ID=your_upkeep_id_here
UPKEEP_NAME=Raffle Auto Draw
UPKEEP_GAS_LIMIT=200000
UPKEEP_FUND_AMOUNT=2000000000000000000

# Anvil test player private keys (for local development only)
# Used by Makefile commands: enter-player-1, enter-player-2, etc.
# Copy these from Anvil output when you run `anvil`
ANVIL_PLAYER_1_KEY=your_anvil_player_1_private_key
ANVIL_PLAYER_2_KEY=your_anvil_player_2_private_key
ANVIL_PLAYER_3_KEY=your_anvil_player_3_private_key
ANVIL_PLAYER_4_KEY=your_anvil_player_4_private_key
ANVIL_PLAYER_5_KEY=your_anvil_player_5_private_key